<% if @custom_view == 'other' %>
  $("#likes-<%= @gadget.id %>").html("<%= escape_javascript(render partial: 'shared/likes', locals: { gadget: @gadget }) %>");
<% elsif @custom_view == 'show' %>
  $("#likes-<%= @gadget.id %>").html("<%= escape_javascript(render partial: 'shared/likes_and_liked_users', locals: { gadget: @gadget }) %>");
<% end %>

